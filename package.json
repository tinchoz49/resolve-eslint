{
  "name": "resolve-eslint",
  "version": "1.0.2",
  "description": "Try to execute a local bin eslint before to execute the global",
  "main": "index.js",
  "bin": {
    "eslint": "index.js"
  },
  "scripts": {
    "test": "ava test.js --verbose",
    "posttest": "eslint .",
    "prepublish": "npm run test",
    "version": "chan release ${npm_package_version} && git add ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinchoz49/resolve-eslint.git"
  },
  "keywords": [
    "eslint",
    "cli",
    "local"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tinchoz49/resolve-eslint/issues"
  },
  "homepage": "https://github.com/tinchoz49/resolve-eslint#readme",
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "@geut/chan": "^1.2.0",
    "ava": "^0.17.0",
    "eslint-config-xo": "^0.17.0"
  },
  "dependencies": {
    "eslint": "^3.12.2",
    "find-up": "^2.1.0"
  },
  "eslintConfig": {
    "extends": "xo/esnext",
    "rules": {
      "indent": [
        "error",
        4
      ]
    }
  }
}
